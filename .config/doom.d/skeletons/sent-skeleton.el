;;; ~/.config/doom.d/skeletons/sent-skeleton.el -*- lexical-binding: t; -*-

(define-skeleton sent-skeleton
  "Inserts a preSENT skeleton into current buffer."
  "Title: "
  "#+TITLE: \n"
  "#+AUTHOR: \n"
  "#+DATE: \n"
  "#+CREATOR: \n"
  "* << EOF\n"
  "title\n"
  "By \n"
  "\n"
  "* I. \n"
  "\n"
  "* II. \n"
  "\n"
  "* III. \n"
  "\n"
  "* IV. \n"
  "\n"
  "* EOF\n"
  "Thanks. 🙏\n"
  "Question? 🤔")
